[defaults]
inventory = /srv/git/cloudbox/inventories/local
library = /srv/git/cloudbox/library
roles_path = roles:/srv/git/cloudbox/roles:/srv/git/cloudbox/resources/roles
filter_plugins = /srv/git/cloudbox/filter_plugins
fact_path = ./ansible_facts.d
log_path = ./community.log
callback_whitelist = profile_tasks
command_warnings = False
retry_files_enabled = False
interpreter_python = /usr/bin/python3
